# Versao do n8n
N8N_VERSION=latest

# Troque por uma string aleatoria com 32+ caracteres (letras maiusculas, minusculas e numeros)
N8N_ENCRYPTION_KEY=altere-para-sua-chave-aleatoria

# IP local para bind da porta do host
BIND_IP=0.0.0.0

# Porta exposta no host
BIND_PORT=5678

# Host/porta usados pelo n8n para gerar URLs
N8N_HOST=localhost
N8N_PORT=5678

# Protocolo do n8n
N8N_PROTOCOL=http

# URL do webhook
WEBHOOK_URL=http://localhost:5678/

# Fuso horario
GENERIC_TIMEZONE=America/Sao_Paulo
TZ=America/Sao_Paulo
